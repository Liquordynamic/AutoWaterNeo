    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.1.0",
    "@radix-ui/react-menubar": "^1.1.2",
    "@types/mapbox-gl": "^3.4.1",
    "@types/proj4": "^2.5.5",
    "@types/uuid": "^10.0.0",
    "adm-zip": "^0.5.16",
    "axios": "^1.7.9",
    "body-parser": "^1.20.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "electron-updater": "^6.1.7",
    "express": "^4.21.2",
    "lucide-react": "^0.468.0",
    "mapbox-gl": "^3.8.0",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "pg": "^8.13.1",
    "proj4": "^2.15.0",
    "react-router": "^7.0.2",
